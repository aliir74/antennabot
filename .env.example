# Telegram Bot Token
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Channel ID or Username (including the @ symbol if it's a username)
TELEGRAM_CHANNEL=@My_Channel

# Enable/Disable YouTube Tutorial Links
ENABLE_TUTORIAL_LINKS=true

# YouTube Tutorial Links (comma-separated if multiple)
YOUTUBE_TUTORIAL_LINKS=https://youtube.com/your_tutorial_link

# Donate Link
DONATE_LINK=https://example.com/donate

# Admin Chat IDs (comma-separated)
ADMIN_CHAT_IDS=123456789,987654321